                {
                  "name": "resetxp",
                  "category": "User Data",
                  "type": "Set User Data",
                  "user": "${tempVars.msguser.id}",
                  "field": "randomxp",
                  "fieldvalue": "0"
                },
                {
                  "name": "addlevel",
                  "category": "User Data",
                  "type": "Edit User Data",
                  "user": "${tempVars.msguser.id}",
